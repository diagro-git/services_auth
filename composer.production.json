{
    "name": "diagro/services_auth",
    "type": "project",
    "description": "Authentication service for Diagro users",
    "keywords": ["diagro", "auth", "services", "laravel"],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/diagro-git/lib_laravel_token.git"
        }
    ],
    "version": "1.6",
    "require": {
        "php": "^8.1",
        "ext-openssl": "*",
        "diagro/lib_laravel_token": "^1.0",
        "doctrine/dbal": "^3.1",
        "firebase/php-jwt": "5.4.0",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.4.0",
        "laravel/framework": "^9.0",
        "laravel/tinker": "^2.6.2",
        "hedii/laravel-gelf-logger": "^7.1",
        "illuminate/log": "^9.0",
        "graylog2/gelf-php": "^1.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "github-oauth": {
            "github.com": "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"
        }
    },
    "minimum-stability": "stable"
}
